    <validationRules>
        <fullName>Check_market</fullName>
        <active>true</active>
        <description>#3279</description>
        <errorConditionFormula>AND(
TEXT(Pricebook2.Market__c)&lt;&gt; TEXT( $User.Market__c ), TEXT(Pricebook2.Market__c)&lt;&gt; TEXT( $User.User_Country__c ),
$User.Profile__c &lt;&gt; &quot;#System: System Administrator&quot;, 
$User.Id &lt;&gt; &quot;00520000001CBYl&quot;,
$User.Profile__c &lt;&gt; &quot;#System: OrgFlow&quot;,
Product2.RecordType.Id &lt;&gt; &apos;0126M000000R5KX&apos;
)</errorConditionFormula>
        <errorMessage>You cannot edit price book entries that are not connected to your market</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NotAllowedToAddInPricebook</fullName>
        <active>true</active>
        <errorConditionFormula>$UserRole.Id  &lt;&gt;  &quot;00E20000000qSiH&quot; &amp;&amp; Pricebook2.ShowPricingfromMicroServiceOM__c
&amp;&amp;
(Pricebook2.CompanyCode__c != Product2.Company_Code__c
||
text(Pricebook2.SourceId__c) != text(Product2.ERP_Source__c))</errorConditionFormula>
        <errorMessage>This Product is not matching Pricebook Company Code or Source</errorMessage>
    </validationRules>
