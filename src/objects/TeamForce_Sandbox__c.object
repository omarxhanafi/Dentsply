        <fullName>Release_Status__c</fullName>
        <description>Used to set the certainty level of release the sandbox will be included to</description>
        <externalId>false</externalId>
        <inlineHelpText>Not Planned = Yet no clue on when to release
Planned = Clear target to meet this release but not yet confirmed it is possible
Confirmed = If nothing unforeseen is coming it will go with the added release</inlineHelpText>
        <label>Release Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Not Planned</fullName>
                    <default>false</default>
                    <label>Not Planned</label>
                </value>
                <value>
                    <fullName>Planned</fullName>
                    <default>false</default>
                    <label>Planned</label>
                </value>
                <value>
                    <fullName>Confirmed</fullName>
                    <default>false</default>
                    <label>Confirmed</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
