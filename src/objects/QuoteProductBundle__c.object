    <nameField>
        <displayFormat>QLB-{0000}</displayFormat>
        <label>Quote Line Bundle #</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Quote Line Bundles</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>doNotAllowQuoteLineBundlesEdit</fullName>
        <active>true</active>
        <description>Certain fields of the Quote Line Bundles cannot be modified</description>
        <errorConditionFormula>AND(
$Setup.GlobalBypassProcessesSettings__c.RunValidationRules__c,
OR (ISCHANGED(Quantity__c), ISCHANGED( CurrencyIsoCode ))
)</errorConditionFormula>
        <errorMessage>Quote Line bundles cannot be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>mustHaveTheSamePricebook</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.GlobalBypassProcessesSettings__c.RunValidationRules__c,
Quote__r.Pricebook2Id &lt;&gt;  ProductBundle__r.Price_Book__c)</errorConditionFormula>
        <errorDisplayField>ProductBundle__c</errorDisplayField>
        <errorMessage>Please select a product bundle associated with the same price book as the quote.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
