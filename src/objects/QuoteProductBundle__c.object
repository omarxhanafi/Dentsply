    <externalSharingModel>ControlledByParent</externalSharingModel>
    <fields>
        <fullName>BundleOfferDiscountAmount__c</fullName>
        <description>Shows the Total Discount Amount of the Bundle or Offer</description>
        <externalId>false</externalId>
        <formula>ProductBundle__r.Total_Discount__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Shows the Total Discount Amount of the Bundle or Offer</inlineHelpText>
        <label>Bundle/Offer Discount Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
