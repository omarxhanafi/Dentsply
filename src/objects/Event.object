    <validationRules>
        <fullName>Associate_w_Contact_Lead_AND_Acc_Opptie</fullName>
        <active>false</active>
        <description>Used by BeNeLux, since they want all CFEs to be associated with both a Contact and an Account/Oppty.</description>
        <errorConditionFormula>AND( Customer_Facing_Event__c = TRUE ,Creator_Market__c = &quot;Benelux&quot;, LEFT(WhoId, 3) &lt;&gt; &quot;003&quot;,OR( LEFT(WhatId,3) &lt;&gt;&quot;001&quot;, LEFT(WhatId,3) &lt;&gt;&quot;006&quot;, LEFT(WhatId,3) &lt;&gt;&quot;a01&quot; ))</errorConditionFormula>
        <errorMessage>Please associate your event with both a Contact/Lead or an Account/Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Associate_w_Contact_Lead_or_Acc_Opptie</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Customer_Facing_Event__c= TRUE, LEN(WhoId )&lt;3,LEN(WhatId)&lt;3)</errorConditionFormula>
        <errorMessage>Please associate your event with either a Contact/Lead or an Account/Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Can_not_mark_future_CFEs_as_completed</fullName>
        <active>true</active>
        <errorConditionFormula>Customer_Facing_Event__c = True &amp;&amp; DATEVALUE(ActivityDateTime) &gt; TODAY() &amp;&amp; ISPICKVAL(Event_Status__c, &quot;Completed&quot;)</errorConditionFormula>
        <errorDisplayField>Event_Status__c</errorDisplayField>
        <errorMessage>You can not mark a future Event as Completed. If the CFE is in fact completed, update the Date before saving</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IN_Not_possible_to_change_passed_CFE</fullName>
        <active>true</active>
        <description>The users should not be able to make changes to any events in the backdate older then 3 days.

Case no : 00487797  // Case: 00627926</description>
        <errorConditionFormula>ActivityDate +3 &lt; Today() &amp;&amp;   ISPICKVAL($User.User_Country__c,&quot;India&quot;) &amp;&amp;  $UserRole.Id &lt;&gt;&quot;00E200000015hlC&quot; &amp;&amp;  $UserRole.Id &lt;&gt;&quot;00E20000000qSiH&quot;</errorConditionFormula>
        <errorMessage>You are not allowed to modify your events occurring 3 days before today. Please contact your CRM Analyst for support.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NA_Activity_TypeSubtype_at_Complete</fullName>
        <active>true</active>
        <description>For USA, Canada, and Indonesia, the Activity Type+Subtype must be filled if it&apos;s a customer-facing event.</description>
        <errorConditionFormula>AND(
	Customer_Facing_Event__c = TRUE
	,
	OR(
		TEXT( $User.User_Country__c) = &quot;USA&quot;,
		TEXT( $User.User_Country__c) = &quot;Canada&quot;,
		TEXT( $User.User_Country__c) = &quot;Indonesia&quot;
	)
	,
	OR(
		ISBLANK(TEXT(Astra_Tech_Type__c )),
		ISBLANK(TEXT(Activity_Subtype__c))
	)
)</errorConditionFormula>
        <errorDisplayField>Activity_Subtype__c</errorDisplayField>
        <errorMessage>Please add an Activity Subtype to this Event.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NA_Associate_w_Contact_Lead_AND_Acc_OptY</fullName>
        <active>false</active>
        <description>Used by NA, since they want all CFEs to be associated with both a Contact and an Account/Oppty.</description>
        <errorConditionFormula>AND( LEN( WhoId) = 0, Customer_Facing_Event__c = TRUE ,Creator_Market__c = &quot;NA&quot;, OR( LEFT(WhatId,3) &lt;&gt;&quot;001&quot;, LEFT(WhatId,3) &lt;&gt;&quot;006&quot;, LEFT(WhatId,3) &lt;&gt;&quot;a01&quot;))</errorConditionFormula>
        <errorDisplayField>WhoId</errorDisplayField>
        <errorMessage>Please associate your event with both a Contact AND an Account/Opportunity/Network!
Name = Contact
Related To = Account OR Opportunity OR Network!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NA_Contact_Required_For_CFE_Completion</fullName>
        <active>false</active>
        <errorConditionFormula>AND( LEN(WhoId)=0,  Customer_Facing_Event__c =TRUE,  TEXT( $User.Market__c) =&quot;NA&quot;, $User.User_SBU__c =&quot;IMPL&quot;,  ISPICKVAL( Event_Status__c ,&quot;Completed&quot;) )</errorConditionFormula>
        <errorDisplayField>WhoId</errorDisplayField>
        <errorMessage>Please add the contact that you met with!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NA_IMP_BUSINESS_INITIATIVE_REQUIRED</fullName>
        <active>false</active>
        <description>If the CFE is completed, business initiative must be filled out</description>
        <errorConditionFormula>AND( $User.User_SBU__c =&quot;IMPL&quot;, 
OR(TEXT($User.User_Country__c)=&quot;Canada&quot;,TEXT($User.User_Country__c)=&quot;USA&quot;), 
Customer_Facing_Event__c =TRUE, 
ISPICKVAL(Event_Status__c , &quot;Completed&quot;), 
AND(INCLUDES(SBU__c,&quot;Implants&quot;), ISBLANK(TEXT(  Business_Initiative__c)  ) ) 
)</errorConditionFormula>
        <errorMessage>Please Select the appropriate Business Initiative for this CFE</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NA_IMP_CFE_Validation_requires_products</fullName>
        <active>false</active>
        <description>The NA IMPL group requires that all CFEs have a &quot;products discussed&quot; filled out for each COMPLETED CFE.</description>
        <errorConditionFormula>AND( $User.User_SBU__c =&quot;IMPL&quot;, 
OR(TEXT($User.User_Country__c)=&quot;Canada&quot;,TEXT($User.User_Country__c)=&quot;USA&quot;), 
Customer_Facing_Event__c =TRUE, 
ISPICKVAL(Event_Status__c , &quot;Completed&quot;), 
AND(INCLUDES(SBU__c,&quot;Implants&quot;), ISBLANK( Call_Report__r.Products_discussed_IMPL__c ) )
 )</errorConditionFormula>
        <errorMessage>PLEASE SELECT &quot;PRODUCTS DISCUSSED&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_editing_of_recurring_event</fullName>
        <active>false</active>
        <description>Prevent editing of recurring events in salesforce which are created in outlook</description>
        <errorConditionFormula>AND( ISCHANGED(Customer_Facing_Event__c) ,Customer_Facing_Event__c = true, isPickVal(EventOrigin__c,&apos;Outlook&apos;) )</errorConditionFormula>
        <errorMessage>Please update the recurring event from outlook</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBU_required_for_CFEs</fullName>
        <active>false</active>
        <description>Validation rule to ensure that at least one SBU is entered for a Customer Facing Event</description>
        <errorConditionFormula>AND(Customer_Facing_Event__c =TRUE,  ISBLANK(SBU__c), ISPICKVAL( Event_Status__c ,&quot;Completed&quot;) ,
$Profile.Name &lt;&gt; &apos;System Administrator&apos;,
$Profile.Name &lt;&gt; &apos;Lightning Sales Rep&apos;,
$Profile.Name &lt;&gt; &apos;Lightning Sales Rep Without Sync&apos;,
$Profile.Name &lt;&gt; &apos;Lightning Inside Sales Rep&apos;
)</errorConditionFormula>
        <errorMessage>When you create a Customer Facing Event you must select a value in the SBU field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subject_validation</fullName>
        <active>false</active>
        <description>Subject must be filled</description>
        <errorConditionFormula>Subject  = &quot;Type Subject here!&quot;
