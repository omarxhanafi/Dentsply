    <validationRules>
        <fullName>You_must_record_call_notes</fullName>
        <active>true</active>
        <description>Checks if the user has entered any call notes before completing the event. Added exception for Lightning profiles as the same logic is build on lightning controllers.</description>
        <errorConditionFormula>AND(
    Customer_Facing_Event__c = TRUE,
    ISPICKVAL(Event_Status__c, &quot;Completed&quot;),
    OR(
        AND(ISBLANK(SBU__c), ISBLANK(Call_Report__r.Call_Notes__c)),
        AND(INCLUDES(SBU__c, &quot;Endodontics&quot;), ISBLANK(Call_Report__r.Call_Notes__c), ISBLANK(Call_Report__r.Products_discussed_ENDO__c)),
        AND(INCLUDES(SBU__c, &quot;Implants&quot;), ISBLANK(Call_Report__r.Call_Notes__c), ISBLANK(Call_Report__r.Products_discussed_IMPL__c)),
        AND(INCLUDES(SBU__c, &quot;Prostethics&quot;), ISBLANK(Call_Report__r.Call_Notes__c), ISBLANK(Call_Report__r.Products_discussed_PROS__c)),
        AND(INCLUDES(SBU__c, &quot;Restorative&quot;), ISBLANK(Call_Report__r.Call_Notes__c), ISBLANK(Call_Report__r.Products_discussed_REST__c)),
        AND(INCLUDES(SBU__c, &quot;Preventive&quot;), ISBLANK(Call_Report__r.Call_Notes__c), ISBLANK(Call_Report__r.Products_discussed_PREV__c)),
        AND(INCLUDES(SBU__c, &quot;Orthodontics&quot;), ISBLANK(Call_Report__r.Call_Notes__c), ISBLANK(Call_Report__r.Products_discussed_ORTH__c)),
        AND(INCLUDES(SBU__c, &quot;CAD/CAM&quot;), ISBLANK(Call_Report__r.Call_Notes__c), ISBLANK(Call_Report__r.Products_discussed_CDCM__c)),
        AND(INCLUDES(SBU__c, &quot;Imaging Systems&quot;), ISBLANK(Call_Report__r.Call_Notes__c), ISBLANK(Call_Report__r.Products_discussed_IMAG__c)),
        AND(INCLUDES(SBU__c, &quot;Instruments&quot;), ISBLANK(Call_Report__r.Call_Notes__c), ISBLANK(Call_Report__r.Products_discussed_INST__c)),
        AND(INCLUDES(SBU__c, &quot;Treatment Centers&quot;), ISBLANK(Call_Report__r.Call_Notes__c), ISBLANK(Call_Report__r.Products_discussed_TRCE__c))
    )
)</errorConditionFormula>
        <errorMessage>Please enter information about your Customer Facing Event either in call notes or what products were discussed.</errorMessage>
    </validationRules>
