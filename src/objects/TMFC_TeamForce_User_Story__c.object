    <validationRules>
        <fullName>Add_cancellation_notes</fullName>
        <active>true</active>
        <description>Forces cancellation notes if a user story is cancelled</description>
        <errorConditionFormula>ISPICKVAL(TMFC_User_Story_Status__c, &quot;Cancelled&quot;)  &amp;&amp;  ISBLANK(Cancellation_reason__c)</errorConditionFormula>
        <errorMessage>You must add cancellation notes before cancelling a User Story</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Add_personas</fullName>
        <active>false</active>
        <description>Forces persona definition if a user story is ready or in production</description>
        <errorConditionFormula>OR(
ISPICKVAL(TMFC_User_Story_Status__c, &quot;In Production - Closed&quot;) ,
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Ready to dev integrate&quot;) , 
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Ready for prod deploy&quot;)) &amp;&amp; 
ISBLANK(Affected_Personas__c)</errorConditionFormula>
        <errorMessage>Please add the personas that are affected by this user story</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Add_release_notes</fullName>
        <active>true</active>
        <description>Forces release notes if a user story is ready or in production</description>
        <errorConditionFormula>OR(
ISPICKVAL(TMFC_User_Story_Status__c, &quot;In Production - Closed&quot;) ,
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Ready to dev integrate&quot;) , 
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Ready for prod deploy&quot;)) &amp;&amp; 
LEN(Release_Notes__c) = 0</errorConditionFormula>
        <errorMessage>You must add release notes for CRM analysts before putting this User Story into production</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Add_result_notes</fullName>
        <active>true</active>
        <description>Forces result notes if a user story is ready or in production</description>
        <errorConditionFormula>OR(
ISPICKVAL(TMFC_User_Story_Status__c, &quot;In Production - Closed&quot;) ,
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Ready to dev integrate&quot;) , 
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Ready for prod deploy&quot;)) &amp;&amp; 
LEN(internal_comment__c) = 0</errorConditionFormula>
        <errorMessage>You must add result notes before putting this User Story into production</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Add_sandbox</fullName>
        <active>true</active>
        <description>A sandbox is required when a development user story progresses to Scheduled</description>
        <errorConditionFormula>NOT(OR(
ISPICKVAL(TMFC_User_Story_Status__c, &quot;New&quot;) ,
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Submitted&quot;) ,
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Grooming&quot;) ,
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Backlog&quot;) ,
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Roadblock - paused&quot;) ,
ISPICKVAL(TMFC_User_Story_Status__c, &quot;Cancelled&quot;))) 
&amp;&amp;
 Sandbox__c  = null
&amp;&amp;
 ISPICKVAL(Area__c, &quot;Development&quot;)</errorConditionFormula>
        <errorMessage>You must add a sandbox before putting this User Story into Scheduled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_change_USstatus_unless_submited</fullName>
        <active>true</active>
        <description>This Validation rule is used to prevent &quot;New&quot; User Stories from being moved to another stage without approval.</description>
        <errorConditionFormula>AND( Text(TMFC_User_Story_Status__c) &lt;&gt; &quot;New&quot;, Submitted_for_Approval__c =FALSE)</errorConditionFormula>
        <errorDisplayField>TMFC_User_Story_Status__c</errorDisplayField>
        <errorMessage>Please keep the status of this User Story as &quot;New&quot; and click the &quot;Submit for Approval&quot; button to assign the User Story to the right Owner/Approver</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
