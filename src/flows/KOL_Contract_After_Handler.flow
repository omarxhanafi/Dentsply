    <description>TFUS-000006878: KOL Contract: After Handler Flow
Set the KOL Contract Name
Send notification 1/2/3 monthsbefore the Contract&apos;s end date</description>
    <environments>Default</environments>
    <formulas>
        <name>IsNew</name>
        <dataType>Boolean</dataType>
        <expression>IsNew()</expression>
    </formulas>
    <formulas>
        <name>KOLContractDefaultName</name>
        <dataType>String</dataType>
        <expression>{!$Record.Account__r.Name} &amp; {!$Record.Contact__r.Full_Name__c} &amp; &apos; &apos; &amp; TEXT({!$Record.End_Date__c}) &amp; &apos; &apos; &amp; {!$Record.KOL_Contract__c}</expression>
    </formulas>
