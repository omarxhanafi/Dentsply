        <defaultConnectorLabel>False</defaultConnectorLabel>
        <rules>
            <name>dec_UKSendSurvey_True</name>
            <conditionLogic>1 AND 2 AND 3 AND 4 AND 5 AND 6 AND ( 7 OR 8) AND (9 OR 10)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
